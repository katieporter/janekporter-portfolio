//- Create template views/page.pug
extends ./layout.pug

//- Create a block
block body
  div(data-wio-id=pageContent.id)
   //- This is how to get an image into your template
   img(src=pageContent.data.featured_image.url)
   //- This is how to get a structured text into your template
   != PrismicDOM.RichText.asHtml(pageContent.data.project_name, ctx.linkResolver)
   != PrismicDOM.RichText.asHtml(pageContent.data.project_title, ctx.linkResolver)
   != PrismicDOM.RichText.asHtml(pageContent.data.body, ctx.linkResolver)
